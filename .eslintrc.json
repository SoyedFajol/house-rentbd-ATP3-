{
    "extends": "next/core-web-vitals",
    "rules": {
      "react/no-unescaped-entities": [
        "error",
        {
          "forbid": [
            {
              "char": "'",
              "alternatives": ["&apos;"]
            }
          ]
        }
      ],
      "@typescript-eslint/no-unused-vars": ["error", {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }]
    }
  }